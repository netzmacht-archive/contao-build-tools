{
    "name":"netzmacht/contao-devtools",
    "description":"Development toolset for Contao extension developing",
    "keywords":["contao", "development", "test", "ci"],
    "type":"library",
    "license":"LGPL-3.0+",
    "authors":[
        {
            "name":"David Molineus",
            "email":"mail@netzmacht.de",
            "homepage":"http://www.netzmacht.de",
            "role":"Project leader"
        }
    ],
    "support":{
        "email":"mail@netzmacht.de",
